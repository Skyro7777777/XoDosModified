    <!-- Aide technique -->
    <string name="dxwrapper_help_content"><![CDATA[
        <i><b>WineD3D</b></i> : Couche de traduction DirectX 1-11 vers OpenGL<br /><br />
        <i><b>DXVK</b></i> : Couche de traduction Direct3D 9-11 vers Vulkan<br /><br />
        <i><b>D8VK</b></i> : Couche de traduction Direct3D 8 vers Vulkan<br /><br />
        <i><b>CNC DDraw</b></i> : Implémentation DirectDraw pour jeux classiques
    ]]></string>

    <string name="box86_64_env_var_help__dynarec_safeflags"><![CDATA[
        Gestion des flags CALL/RET<br /><br />
        <b>0</b> : Ignorer les flags (rapide, instable)<br />
        <b>1</b> : Flags pour RET seulement<br />
        <b>2</b> : Flags systématiques (lent)
    ]]></string>

    <string name="box86_64_env_var_help__dynarec_fastnan">Génération de -NAN x86</string>
    <string name="box86_64_env_var_help__dynarec_fastround">Arrondi précis x86</string>
    <string name="box86_64_env_var_help__dynarec_x87double">Utilisation Float/Double</string>
    <string name="box86_64_env_var_help__dynarec_bigblock"><![CDATA[
        Taille des blocs Dynarec<br /><br />
        <b>0</b> : Blocs standards<br />
        <b>1</b> : Blocs étendus<br />
        <b>2</b> : Blocs ELF étendus<br />
        <b>3</b> : Blocs illimités
    ]]></string>

    <string name="box86_64_env_var_help__dynarec_strongmem"><![CDATA[
        Modèle mémoire strict<br /><br />
        <b>0</b> : Désactivé<br />
        <b>1</b> : Barrières partielles<br />
        <b>2</b> : Barrières complètes<br />
        <b>3</b> : Barrières étendues
    ]]></string>

